<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.navigatorsguide.app.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.navigatorsguide.app.ui.gallery.ProfileFragment"
        android:label="@string/menu_nav_2"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/nav_report"
        android:name="com.navigatorsguide.app.ui.report.ReportBuilderFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_report_builder" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.navigatorsguide.app.ui.slideshow.SlideshowFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_slideshow" />
</navigation>